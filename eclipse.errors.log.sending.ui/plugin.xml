<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="eclipse.errors.log.sending.ui.helpMenu"
         name="HelpMenu"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:help">
         <command
               commandId="eclipse.errors.log.sending.ui.sendCommand"
               id="eclipse.errors.log.sending.ui.sendCommand"
               label="Отправить отчёт об ошибке"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         id="eclipse.errors.log.sending.ui.helpCommands"
         name="HelpCommands"
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="eclipse.errors.log.sending.ui.SendCommand"
            description="Отправить отчёт об ошибке"
            id="eclipse.errors.log.sending.ui.sendCommand"
            name="sendCommand">
      </command>
   </extension>

</plugin>
